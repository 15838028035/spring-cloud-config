# Auto Configure
org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
org.springframework.platform.autoconfigure.RefreshAutoConfiguration

# Application Listeners
org.springframework.context.ApplicationListener=\
org.springframework.platform.bootstrap.BootstrapApplicationListener,\
org.springframework.platform.context.restart.RestartListener

# Bootstrap components
org.springframework.platform.bootstrap.BootstrapConfiguration=\
org.springframework.platform.bootstrap.config.ConfigServiceBootstrapConfiguration,\
org.springframework.boot.autoconfigure.PropertyPlaceholderAutoConfiguration